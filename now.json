{
  "version": 2,
  "builds": [
    {
      "src": "packages/mini_ui-react/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "examples/gatsby_example/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/mini-gatsby-theme/(.*)",
      "dest": "/examples/gatsby_example/$1"
    },
    {
      "src": "^/mini-ui-react/(.*)",
      "dest": "/packages/mini_ui-react/$1"
    }
  ],
  "trailingSlash": false,
  "build": {
    "env": {
      "NPM_RC": "@npmrc"
    }
  }
}
